    "i18next-vue": "^5.3.0",
    "markdown-it": "^14.1.0",
    "plotly.js": "^3.3.0",
    "plotly.js-dist-min": "^3.3.0",
    "vee-validate": "^4.15.1",
    "vue": "^3.3.4",
    "vue-plotly": "^1.1.0",
    "vue-router": "^4.2.5",
    "vuetify": "^3.10.11"
